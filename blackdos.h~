#define PRINTS(x) interrupt(33,0,x,0,0)
#define PRINTN(x) interrupt(33,13,x,0,0)
#define SCANS(x)  interrupt(33,1,x,0,0)
#define SCANN(x)  interrupt(33,14,&x,0,0)
#define END       interrupt(33,5,0,0,0)
#define STRCMP(x,y) {\
   while(*str1 != '\0')\
   {\
      if(*str1 != *str2)\
         return 0;\
      *str1++;\
      *str2++;\
   }\
   return 1;\
}
